# Redwood demo local env (Node dev server)
# Provider options: mock | openai | openrouter
AI_PROVIDER=mock

# OpenAI
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini

# OpenRouter
OPENROUTER_API_KEY=
OPENROUTER_MODEL=openai/gpt-4o-mini

# Optional telemetry to stdout
CHAT_TELEMETRY_STDOUT=0

# Optional R2 public URL (if using R2 binding)
R2_PUBLIC_BASE_URL=
R2_PREFIX=redwood-chat-system

# Optional demo server port
DEMO_PORT=8910
